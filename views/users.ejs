<body>
    <%- include ("partials/navbar") -%>
    <section id="view complaint">
        
        <div class="viewc">
            
   
   <h1>Users</h1>
   <table>
    <tr>
      <th colspan="2" > name</th>
     
      <!-- <th style="width:20%">Complaint type</th> -->
      <th>house number</th>
      <th>Floor number</th>
      <!-- <th>Date</th>
      <th style="width:30%">description</th> -->
      <th colspan="2">Monthly Due</th>

    </tr>
    <% arrs.forEach(function(arr){ %>
    <tr>
      <td><%= arr.fname %></td>
      <td><%= arr.lname %></td>
      <!-- <td><%= arr.cType %></td> -->
      <td> <%= arr.housenumber %></td>
      <td><%= arr.Floor_number %></td>
      <!-- <td><%= arr.cDate %></td>
      <td> <%= arr.cDescription %></td> -->
      <td><%= arr.due %></td>
      <td>
      
          <!-- <a href="/updateuser?id=<%= arr._id%>"  -->
             <a class="btn btn-primary" href="/updateusers/<%= arr._id%>" role="button">ADD</a>
      </td>
      
    </tr>
    <% }) %> 
    
  </table>


        </div>
        
  
  </body>
