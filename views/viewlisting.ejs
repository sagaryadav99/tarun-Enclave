<body>
    <%- include ("partials/navbar") -%>
    <section id="view Complaint">
        
        <div class="viewc">
            
   
   <h1>Properties</h1>
   <table>
    <tr>
      <th colspan="2" > name</th>
     
      <!-- <th style="width:20%">Complaint type</th> -->
      <th>house number</th>
      <th>Floor number</th>
      <th>Contact info</th>
      <th>Date</th>
      <th style="width:30%">description</th>

    </tr>
    <% arrs.forEach(function(arr){ %>
    <tr>
      <td><%= arr.bFname %></td>
      <td><%= arr.bLname %></td>
      <td> <%= arr.bHouseNumber %></td>
      <td><%= arr.bFloorNumber %></td>
      <td><%= arr.bPhoneNumber %></td>
      <td><%= arr.bDate %></td>
      <td> <%= arr.bDescription %></td>
      
    </tr>
    <% }) %> 
    
  </table>
  <!-- <button class="w-100 btn btn-lg btn-primary" href="/buysell.ejs">Add property</button> -->
  <a class="btn btn-primary" href="/buysell" role="button">Add property</a>


        </div>
        
  
  </body>
